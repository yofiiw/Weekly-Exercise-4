nilai = float(input("Masukkan Nilai : "))
if nilai >= 90:
    print("Grade A")
elif nilai >= 80:
    print("Grade B+")
elif nilai >= 70:
    print("Grade B")
elif nilai >= 60:
    print("Grade C+")
elif nilai >= 50:
    print("Grade C")
elif nilai >= 40:
    print("Grade D")
else:
    print("Grade E")
